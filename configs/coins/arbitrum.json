        "mainnet": true,
        "server_config_file": "",
        "client_config_file": ""
    },
    "blockbook": {
        "package_name": "blockbook-arbitrum",
        "system_user": "blockbook-arbitrum",
        "internal_binding_template": ":{{.Ports.BlockbookInternal}}",
        "public_binding_template": ":{{.Ports.BlockbookPublic}}",
        "explorer_url": "",
        "additional_params": "",
